{
  "name": "healthone",
  "description": "Health One format library for JavaScript",
  "version": "2.0.0",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/dermatodoc/healthone/issues"
  },
  "dependencies": {
    "@aureooms/js-cardinality": "^2.0.0",
    "@aureooms/js-itertools": "^4.0.0",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.0.10",
    "faker": "^4.1.0",
    "lodash.mergewith": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-async-generator-functions": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "ava": "3.8.2",
    "chardet": "^1.1.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "iconv-lite": "^0.6.0",
    "np": "6.2.4",
    "nyc": "15.1.0",
    "xo": "0.32.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://dermatodoc.github.io/healthone",
  "keywords": [
    "Health",
    "One",
    "format",
    "parser"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dermatodoc/healthone"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "xo && ava"
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib/**",
      "doc/**"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    }
  }
}
